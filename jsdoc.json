{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "assets/js/app.js"
        ],
        "exclude": [
            "node_modules"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./documentacion/",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}